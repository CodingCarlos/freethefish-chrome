{
  "name": "[exp] Clickbait",
  "version": "0.1.1",
  "description": "Spoilers for clickbaits",
  "permissions": [
    "storage",
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
   {
     "matches": ["<all_urls>"],
     "css": ["clickbait.css"],
      "js": ["clickbait.js"]
   }
 ],
  "manifest_version": 2,
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" }
}
